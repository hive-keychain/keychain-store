{
  "name": "keychainStore",
  "version": "0.5.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-bundle": "rm -rf android/app/build/outputs/bundle/* && cd android && ./gradlew bundleRelease && cd ..",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "dep-clean": "rm -rf node_modules && npm install --legacy-peer-deps",
    "clean": "watchman watch-del-all &&npm run dep-clean && npm run android-clean && npm run start -- --reset-cache",
    "bump": "npx react-native bump-version --type patch",
    "bump:minor": "npx react-native bump-version --type minor"
  },
  "dependencies": {
    "@hiveio/dhive": "^1.2.7",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "hive-uri": "^0.2.4",
    "i18next": "^23.4.5",
    "moment": "^2.29.4",
    "native-base": "^3.4.28",
    "react": "^18.3.1",
    "react-i18next": "^13.2.0",
    "react-native": "^0.72.17",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "3.4",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.24.0",
    "react-native-share": "^11.0.4",
    "react-native-svg": "^12.5.1",
    "react-native-vector-icons": "^9.2.0",
    "rn-qr-generator": "^1.3.1",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.3.1",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.9",
    "prettier": "^2.4.1",
    "react-native-cli-bump-version": "^1.5.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
