{
  "expo": {
    "name": "Keychain Store",
    "slug": "keychain-store",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "translucent": false,
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keychainstore"
    },
    "android": {
      "package": "com.keychainstore",      
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher_foreground.png",
        "backgroundColor": "#000"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ic_launcher_foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a20379e-a93a-4612-a155-9e8dd8efe056"
      }
    }
  }
}
